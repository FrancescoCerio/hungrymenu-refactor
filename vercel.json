{
  "buildCommand": "node scripts/set-env.js && npm run build -- --configuration production",
  "outputDirectory": "www/browser",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/keep-alive",
      "schedule": "0 0 * * *"
    }
  ]
}
